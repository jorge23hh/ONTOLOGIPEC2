@prefix : <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6#> .

<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6> rdf:type owl:Ontology ;
                                                                             rdfs:comment """This ontology is created with the aim of analysing and representing different factors and aspects that encompass the satisfaction of passengers surveyed by a flight.

Created by Jorge Rodríguez Torres and Paula Calero Adalid""" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6#isRuleEnabled
:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasAirline
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasAirline> rdf:type owl:ObjectProperty ,
                                                                                                owl:AsymmetricProperty ;
                                                                                       rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Review> ;
                                                                                       rdfs:range <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Airline> ;
                                                                                       rdfs:comment "indicates which airline a rating refers to" ;
                                                                                       rdfs:label "hasAirline" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasCountry
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasCountry> rdf:type owl:ObjectProperty ,
                                                                                                owl:FunctionalProperty ,
                                                                                                owl:AsymmetricProperty ;
                                                                                       rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Passenger> ;
                                                                                       rdfs:range <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Country> ;
                                                                                       rdfs:comment "passenger's country" ;
                                                                                       rdfs:label "hasCountry" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasDatePublished
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasDatePublished> rdf:type owl:ObjectProperty ,
                                                                                                      owl:FunctionalProperty ,
                                                                                                      owl:AsymmetricProperty ;
                                                                                             rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Review> ;
                                                                                             rdfs:range <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/DatePublished> ;
                                                                                             rdfs:comment "refers to the date on which a valuation is published." ;
                                                                                             rdfs:label "hasDatePublished" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasSeat
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasSeat> rdf:type owl:ObjectProperty ,
                                                                                             owl:FunctionalProperty ,
                                                                                             owl:AsymmetricProperty ;
                                                                                    rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Review> ;
                                                                                    rdfs:range <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/SeatType> ;
                                                                                    rdfs:comment "Passenger who made a review" ;
                                                                                    rdfs:label "hasSeat" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasTypeLeisure
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasTypeLeisure> rdf:type owl:ObjectProperty ,
                                                                                                    owl:FunctionalProperty ,
                                                                                                    owl:AsymmetricProperty ;
                                                                                           rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Passenger> ;
                                                                                           rdfs:range <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Leisure> ;
                                                                                           rdfs:comment "indicates the type of leisure travel undertaken by a passenger, classified as a couple, single or family." ;
                                                                                           rdfs:label "hasTypeLeisure" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasTypeSeat
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasTypeSeat> rdf:type owl:ObjectProperty ,
                                                                                                 owl:FunctionalProperty ,
                                                                                                 owl:AsymmetricProperty ;
                                                                                        rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Passenger> ;
                                                                                        rdfs:range <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/SeatType> ;
                                                                                        rdfs:comment "indicates the type of seat a passenger has, it can be economy, business or first class." ;
                                                                                        rdfs:label "hasTypeSeat" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6#hasRating
:hasRating rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :SatisfactionRating ;
           rdfs:range xsd:integer ;
           rdfs:comment "numerical evaluation" ;
           rdfs:label "hasRating" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasAPublicationDate
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasAPublicationDate> rdf:type owl:DatatypeProperty ,
                                                                                                         owl:FunctionalProperty ;
                                                                                                rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/DatePublished> ;
                                                                                                rdfs:range xsd:date ;
                                                                                                rdfs:comment "indicates the date of publication of the review" ;
                                                                                                rdfs:label "hasAPublicationDate" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasDestination
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasDestination> rdf:type owl:DatatypeProperty ,
                                                                                                    owl:FunctionalProperty ;
                                                                                           rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Destination> ;
                                                                                           rdfs:range xsd:string ;
                                                                                           owl:propertyDisjointWith <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasOrigin> ;
                                                                                           rdfs:comment "indicates the destination city of a flight" ;
                                                                                           rdfs:label "hasDestination" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasOrigin
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasOrigin> rdf:type owl:DatatypeProperty ,
                                                                                               owl:FunctionalProperty ;
                                                                                      rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Origin> ;
                                                                                      rdfs:range xsd:string ;
                                                                                      rdfs:comment "indicates the city of origin of a flight" ;
                                                                                      rdfs:label "hasOrigin" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasTitle
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasTitle> rdf:type owl:DatatypeProperty ,
                                                                                              owl:FunctionalProperty ;
                                                                                     rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Title> ;
                                                                                     rdfs:range xsd:string ;
                                                                                     rdfs:comment "indicates the title of a review" ;
                                                                                     rdfs:label "hasTitle" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasVerification
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasVerification> rdf:type owl:DatatypeProperty ,
                                                                                                     owl:FunctionalProperty ;
                                                                                            rdfs:domain <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Verification> ;
                                                                                            rdfs:range xsd:boolean ;
                                                                                            rdfs:comment "tells us whether a review is verified or not." ;
                                                                                            rdfs:label "hasVerification" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6#SatisfactionRating
:SatisfactionRating rdf:type owl:Class ;
                    rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Rating> ;
                    rdfs:comment "rating that assesses customer satisfaction, low, high or medium, by means of a survey." ;
                    rdfs:label "SatisfactionRating" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Airline
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Airline> rdf:type owl:Class ;
                                                                                    rdfs:comment "Airline on which the survey is conducted" ;
                                                                                    rdfs:label "Airline" ;
                                                                                    rdfs:sameAs "https://dbpedia.org/page/Airline" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Author
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Author> rdf:type owl:Class ;
                                                                                   rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Review> ;
                                                                                   rdfs:comment "is the name of the author of the review" ;
                                                                                   rdfs:label "Author" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Bussines
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Bussines> rdf:type owl:Class ;
                                                                                     rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/TypePassenger> ;
                                                                                     owl:disjointWith <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Leisure> ;
                                                                                     rdfs:comment "type of business passenger" ;
                                                                                     rdfs:label "Bussiness" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Connectivity
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Connectivity> rdf:type owl:Class ;
                                                                                         rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Service> ;
                                                                                         owl:disjointWith <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Entertaiment> ,
                                                                                                          <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/SeatConfort> ;
                                                                                         rdfs:comment "numerical assessment of the connectivity services offered by the airline" ;
                                                                                         rdfs:label "Connectivity" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Country
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Country> rdf:type owl:Class ;
                                                                                    rdfs:comment "country of passenger surveyed" ;
                                                                                    rdfs:label "Country" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Couple
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Couple> rdf:type owl:Class ;
                                                                                   rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Leisure> ;
                                                                                   owl:disjointWith <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Family> ,
                                                                                                    <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Solo> ;
                                                                                   rdfs:comment "type of leisure passenger as a couple" ;
                                                                                   rdfs:label "Couple" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/DatePublished
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/DatePublished> rdf:type owl:Class ;
                                                                                          rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Review> ,
                                                                                                          [ rdf:type owl:Restriction ;
                                                                                                            owl:onProperty <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/hasDatePublished> ;
                                                                                                            owl:someValuesFrom <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Review>
                                                                                                          ] ;
                                                                                          rdfs:comment "is the date of the review" ;
                                                                                          rdfs:label "DatePublished" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Destination
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Destination> rdf:type owl:Class ;
                                                                                        rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Location> ;
                                                                                        owl:disjointWith <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Origin> ;
                                                                                        rdfs:comment "Final destination of flight" ;
                                                                                        rdfs:label "Destination" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Dissatisfied
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Dissatisfied> rdf:type owl:Class ;
                                                                                         rdfs:subClassOf :SatisfactionRating ;
                                                                                         owl:disjointWith <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/PartiallySatisfied> ,
                                                                                                          <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Satisfied> ;
                                                                                         rdfs:comment "low numerical customer rating of the services provided" ;
                                                                                         rdfs:label "Dissatisfied" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Economic
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Economic> rdf:type owl:Class ;
                                                                                     rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/SeatType> ;
                                                                                     owl:disjointWith <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Executive> ,
                                                                                                      <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/FirstClass> ;
                                                                                     rdfs:comment "economic seat type" ;
                                                                                     rdfs:label "Economic" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Entertaiment
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Entertaiment> rdf:type owl:Class ;
                                                                                         rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Service> ;
                                                                                         owl:disjointWith <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/SeatConfort> ;
                                                                                         rdfs:comment "numerical assessment of the entertainment services offered by the airline" ;
                                                                                         rdfs:label "Entertaiment" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Executive
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Executive> rdf:type owl:Class ;
                                                                                      rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/SeatType> ;
                                                                                      owl:disjointWith <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/FirstClass> ;
                                                                                      rdfs:comment "type of executive seat" ;
                                                                                      rdfs:label "Executive" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Family
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Family> rdf:type owl:Class ;
                                                                                   rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Leisure> ;
                                                                                   owl:disjointWith <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Solo> ;
                                                                                   rdfs:comment "type of family leisure passenger" ;
                                                                                   rdfs:label "Family" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/FirstClass
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/FirstClass> rdf:type owl:Class ;
                                                                                       rdfs:subClassOf <http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/SeatType> ;
                                                                                       rdfs:comment "first class seat type" ;
                                                                                       rdfs:label "FirstClass" .


###  http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Flight
<http://www.semanticweb.org/jorgestu/ontologies/2024/3/untitled-ontology-6/Flight> rdf:type owl:Class ;
